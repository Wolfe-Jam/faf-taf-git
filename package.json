{
  "name": "faf-taf-git",
  "version": "1.0.3",
  "description": "Platform-agnostic TAF (Testing Activity Feed) updater - works in ANY CI/CD",
  "main": "dist/cli.js",
  "bin": {
    "faf-taf-git": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "package": "ncc build src/index.ts -o dist --source-map --license licenses.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wolfe-Jam/faf-taf-git.git"
  },
  "keywords": [
    "testing",
