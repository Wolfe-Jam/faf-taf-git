    "ci-cd",
    "github-actions",
    "gitlab-ci",
    "bitbucket-pipelines",
    "test-automation",
    "jest",
    "faf",
    "git",
    "platform-agnostic"
  ],
  "author": "wolfejam",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@vercel/ncc": "^0.38.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
